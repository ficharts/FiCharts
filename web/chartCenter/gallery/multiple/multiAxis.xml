<config title='2010年产品销量'>
    <axis>
        <x type="field" title='月份'/>
        <y id='a' type="linear" title='产量'>
            <label text='${value}K'>
                <format color='${color}'/>
            </label>
            <title>
                <format color='${color}'/>
            </title>
        </y>
        <y id='b' type="linear" title='销售额'>
            <label text='${value}{useGrouping: true}万'>
                <format color='${color}'/>
            </label>
            <title>
                <format color='${color}'/>
            </title>
        </y>
    </axis>
    <series>
        <line seriesName="产品A"  xField='label' yField='valueA' yAxis='a'/>
        <column seriesName="产品B"  xField='label' yField='valueB' yAxis='b'/>
    </series>
    <legend enable='true'/>
    <data>
        <set label='1月' valueA='279' valueB='4000'/>
        <set label='2月' valueA='410' valueB='2000'/>
        <set label='3月' valueA='325' valueB='1500'/>
        <set label='4月' valueA='572' valueB='1000'/>
        <set label='5月' valueA='629' valueB='3000'/>
        <set label='6月' valueA='710' valueB='4000'/>
        <set label='7月' valueA='489' valueB='6000'/>
        <set label='8月' valueA='635' valueB='8000'/>
        <set label='9月' valueA='750' valueB='6000'/>
        <set label='10月' valueA='600' valueB='4000'/>
        <set label='11月' valueA='760' valueB='3500'/>
        <set label='12月' valueA='950' valueB='3000'/>
    </data>
</config>